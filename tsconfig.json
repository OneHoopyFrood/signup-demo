{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "outDir": "./dest",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "include": ["src/**/*.ts", "src/**/*.tsx"],
  "exclude": ["node_modules"]
}
